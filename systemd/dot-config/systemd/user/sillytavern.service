[Unit]
Description=Start SillyTavern service.
After=default.target

[Service]
Type=simple
ExecStart=/home/petid/Repos/SillyTavern/start.sh
Environment="SILLYTAVERN_SELENIUM_BROWSER=firefox"
# WorkingDirectory=/home/petid/Repos/SillyTavern
#
Restart=on-failure
RestartSec=5s

[Install]
WantedBy=default.target

